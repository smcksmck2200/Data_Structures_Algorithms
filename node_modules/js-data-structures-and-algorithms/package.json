  "module": "dist/main.esm.min.js",
  "browser": "dist/main.umd.min.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thawkin3/js-data-structures-and-algorithms.git"
  },
  "keywords": [
    "algorithm",
    "algorithm-concepts",
